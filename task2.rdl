  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>Keys</DataSourceName>
        <CommandText>

SELECT  p.[Name],op.OwnerId,CONCAT_WS('',p1.FirstName,' ',p1.MiddleName) as FullName,op.[PropertyId],tp.[PaymentAmount],tpf.Code,pe.Description as Expenses, pe.date, pe.amount,CONCAT_WS('',ad.number,' ',ad.street,' ',ad.suburb,' ',ad.city) AS PrpertyAddress,p.bedroom,p.bathroom
FROM [dbo].[Property] p
LEFT JOIN  [dbo].[OwnerProperty] op ON p.[Id]= op.propertyId
LEFT JOIN  [dbo].[TenantProperty] tp ON p.[Id]= tp.propertyId
LEFT JOIN [dbo].[TenantPaymentFrequencies] tpf ON tpf.Id = tp.[PaymentFrequencyId]
LEFT JOIN  [dbo].[PropertyExpense] pe ON p.id = pe.[PropertyId]
LEFT JOIN  [dbo].[Address] ad ON ad.AddressId = p.AddressId
LEFT JOIN [dbo].[Person] p1 ON p1.Id = op.OwnerId

WHERE p.[Name] = 'Property A'</CommandText>
      </Query>
